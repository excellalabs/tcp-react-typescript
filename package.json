{
  "name": "tcp-app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@date-io/date-fns": "^1.3.13",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "4.0.3",
    "@material-ui/core": "^4.12.2",
    "@material-ui/pickers": "^3.3.10",
    "typescript": "^4.3.5",
    "web-vitals": "^1.1.2",
    "axios": "^0.21.1",
    "date-fns": "^2.23.0",
    "formik": "^2.2.9",
    "jwt-decode": "^2.2.0",
    "react-router-dom": "^5.2.0",
    "yup": "^0.29.3"
  },
  "proxy": "http://host.docker.internal:3000",
  "scripts": {
    "start": "PORT=9000 react-scripts start",
    "build": "react-scripts build",
    "eject": "react-scripts eject",
    "test": "react-scripts test --env=jest-environment-jsdom-sixteen --resetMocks=false",
    "test:coverage": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --watchAll=false --resetMocks=false",
    "test:ci": "react-scripts test --env=jest-environment-jsdom-sixteen --coverage --runInBand --resetMocks=false",
    "test:cypress": "cypress run",
    "test:cypress:open": "cypress open",
    "format": "prettier --write .",
    "format:check": "prettier --list-different .",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "restoreMocks": true,
    "clearMocks": true,
    "resetMocks": true
  },
  "devDependencies": {
    "@testing-library/cypress": "^8.0.0",
    "@testing-library/jest-dom": "^5.14.1",
    "@testing-library/react": "^11.2.7",
    "@testing-library/user-event": "^12.8.3",
    "@types/jest": "^26.0.24",
    "@types/jwt-decode": "^2.2.1",
    "@types/lodash": "^4.14.172",
    "@types/node": "^14.5.0",
    "@types/react": "^17.0.16",
    "@types/react-dom": "^17.0.9",
    "@types/react-router-dom": "^5.1.8",
    "@types/yup": "^0.29.13",
    "cypress": "^8.2.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-cypress": "^2.11.3",
    "eslint-plugin-jest": "^24.4.0",
    "eslint-plugin-prettier": "^3.4.0",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "msw": "^0.33.3",
    "prettier": "^2.3.2",
    "rimraf": "^3.0.2"
  }
}
